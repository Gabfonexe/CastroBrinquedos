            <script>
    function termoDeAceiteTrilha(ofertaId, pessoaId) {
        $.ajax({
            url: hostname + 'index/getDatasOfertaTrilha',
            type: 'POST',
            dataType: 'json',
            async: true,
            data: {ofertaId: ofertaId},
            error: function() {
                alert('Problemas ao buscar informações da Trilha.');
                callback();
            },
            success: function(data) {
                if (data == null || data.dataInicio == null || data.dataFim == null) {
                    alert('Problemas ao buscar informações da Trilha.');
                    callback();
                }

                limpaDadosTermoAceite()
                $("#dataInicioOfertaTrilha").text(data.dataInicio)
                $("#dataFimOfertaTrilha").text(data.dataFim)
                $('#modalTrilhaCarreira').modal('hide')
                $('#termoAceiteCarreira').modal('show')
                $('#ofertaId').val(ofertaId)
                $('#pessoaId').val(pessoaId)
            }
        });
    }

    function limpaDadosTermoAceite() {
        $("#dataInicioOfertaTrilha").text('');
        $("#dataFimOfertaTrilha").text('');
        $('#ofertaId').val('')
        $('#pessoaId').val('')
        $('#aceitarTermos').prop("disabled", true);
        $('#aceiteTermoTrilha').attr("checked", false);
    }

    $('#aceiteTermoTrilha').on('change', function(e) {
        if (this.checked) {
            $('#aceitarTermos').prop("disabled", false);
        } else {
            $('#aceitarTermos').prop("disabled", true);
        }
    });

    function salvarSolicitacaoMatriculaTrilha() {
        var ofertaId = $('#ofertaId').val();
        var pessoaId = $('#pessoaId').val();

        $.ajax({
            url: hostname + 'index/salvarSolicitacaoMatriculaTrilha',
            type: 'POST',
            dataType: 'json',
            async: true,
            data: {ofertaId: ofertaId, pessoaId: pessoaId},
            error: function() {
                alert('Problemas ao solicitar matrícula.');
                callback();
            },
            success: function(data) {
                alert('Estamos construindo seu acesso, em instantes será liberado.');
                $('#modalTrilhaCarreira').modal('hide');
                reload();
            }
        });
    }

    function reload() {
        document.location.reload(true);
    }

</script>


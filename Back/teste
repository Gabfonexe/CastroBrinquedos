          <script>
                function verificaCookiesAluno() {

                    if(sessionStorage.getItem("verificacookies") === "true"){
                        $('#divCookiesAluno').addClass('escondeDivCookies');
                        $('#divCookiesAluno').removeClass('mostraDivCookies');
                    } else {
                        $('#divCookiesAluno').show();
                        sessionStorage.setItem("verificacookies","false");
                    }

                }

                $('#btnCookiesAluno').on('click', function () {
                    $('#divCookiesAluno').addClass('escondeDivCookies');
                    $('#divCookiesAluno').removeClass('mostraDivCookies');
                    sessionStorage.setItem("verificacookies","true");
                });


                window.onload = function() {
                    verificaCookiesAluno();
                }

            </script>


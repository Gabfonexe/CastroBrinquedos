    <script>
    
    

    

    function limparTodos() {
        $('#listaManuaisDinamico > li ').each(function () {
            this.remove();
        });
    }

    function populaMateriais(data) {
        $.each(data.listaMateriais, function (key, val) {

            var ul = document.getElementById("listaManuaisDinamico");
            var li = document.createElement("li");
            var a = document.createElement("a");
            var text = document.createTextNode(val.descricao);
            a.setAttribute("href", val.valor);
            a.setAttribute("target", '_blank');
            a.appendChild(text);
            li.appendChild(a);
            ul.appendChild(li);
        });
    }

    function checkDashboard() {
        if(window.location.href.indexOf('dashboard') > 0) {
            showModalTour('forceTour');
        } else {
            alert('Para iniciar o Tour Virtual, acesse o curso desejado e clique novamente neste menu.');
        }
    }
</script>


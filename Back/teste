            <script>
		function verificaTabelaTrilhaCarreira() {
		    return $('#tabelaCursoTrilha > tbody > tr').length
		}

        function abrirModalTrilhaDeCarreira(matriculaList) {
            $('#modalTrilhaCarreira').modal('show');

            if (verificaTabelaTrilhaCarreira()) {
                return false;
			}

            $.ajax({
                url: hostname + "index/getCursosTrilha",
                method: "POST",
                dataType: 'json',
				data: { matriculaList: matriculaList },
                success: function(result) {
                    $('#modalTrilhaCarreira').modal('show');
					if (result.cursosTrilhaList.length > 0) {
					    var cursoList = result.cursosTrilhaList;
						$(cursoList).each( function (index, cursoTrilha) {
							//Monta linhas da trilha.
							var elementoTr = constroiTr(cursoTrilha);
							$('#mensagemDeCarregamentoCursosTrilha').hide();
							$('#tabelaCursoTrilha > tbody').append(elementoTr);
						})
					} else {
					    $('#mensagemDeCarregamentoCursosTrilha').html('');
					    $('#mensagemDeCarregamentoCursosTrilha').html('<h6 class="modal-title" style="text-align: center; font-weight: bold !important;">Você não possui nenhuma trilha cadastrada!</h6>');
					}

                },
                error: function(result){

                }
            });
        }

        function constroiTr(cursoTrilha) {
            var matriculaGerada = matriculaFoiGerada(cursoTrilha);
			var finalizouGeracaoMatricula = matriculaEmGeracao(cursoTrilha);
			var pessoaId = $('#pessoaIdTrilhaCarreira').val();

			var botao = matriculaGerada ?
                '<button type="submit" class="btn btn-success" onclick="termoDeAceiteTrilha('+ cursoTrilha.OFERTAID +', '+ pessoaId +')">' +
                	' <i class="fa fa-plus-square-o" aria-hidden="true"></i> Iniciar' +
                '</button>'
        		: (finalizouGeracaoMatricula ?
					'<a class="btn btn-primary" href="/aluno/dashboard/index/'+ cursoTrilha.MATRICULA +'"><i class="fa fa-sign-in" aria-hidden="true" title="Entrar curso'+ cursoTrilha.NOMETRILHA +'"></i> Entrar </a>'
					: 	'<button type="submit" class="btn btn-warning" onclick="reload()">' +
                    		'<i class="fa fa-spinner" aria-hidden="true"></i> Aguardando' +
                    	'</button>');

			return "<tr>" +
				"<td>"+ cursoTrilha.NOMETRILHA +"</td>" +
				"<td>" + botao + "</td>" +
			"</tr>";
		}

		function matriculaFoiGerada(cursoTrilha) {
            return !cursoTrilha.MATRICULA && !cursoTrilha.GERACAO
		}

		function matriculaEmGeracao(cursoTrilha) {
			return !!cursoTrilha.MATRICULA
		}

        function abrirModalExtensao(matriculaList) {
            $('#modalCursoExtensao').modal('show');

		}
	</script>


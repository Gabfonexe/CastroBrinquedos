        <script>
    var disciplinasAnotacoes = [];
    function clearListDisciplinas() {
        disciplinasAnotacoes = [];
        var lista = document.getElementById("anotacoes-disciplinas-list")
        lista.innerHTML = ""
    }
    function openAnotacoesListDisciplinas() {
        const matriculaId = document.getElementById("anotacoes-matricula-id").value

        if (disciplinasAnotacoes.length == 0) {
            $.ajax({
                url: hostname + "aluno/anotacao/disciplinas",
                type: 'GET',
                dataType: 'json',
                data: {matriculaId: matriculaId},
                success: function (retorno) {
                    disciplinasAnotacoes = retorno;
                    clearListDisciplinas();
                    var lista = document.getElementById("anotacoes-disciplinas-list");
                    retorno.forEach(function (el) {
                        lista.innerHTML +=
                            "<li>" +
                                "<a href=\"#\" onclick=\"openAnotacoesFormulario('" + el.DISCIPLINA_ID + "', '" + el.DISCIPLINA_DESCRICAO + "');return false\">" +
                                    "<p>" + el.DISCIPLINA_DESCRICAO + "</p>" +
                                    "<i class='fa fa-play'></i>" +
                                "</a>" +
                            "</li>";
                    });
                    openAnotacoesBody("body-anotacoes-list-disciplinas");
                },
                error: function (retorno) {
                    console.log('error', retorno);
                    openAnotacoesBody('body-anotacoes-erro');
                    clearListDisciplinas()
                }
            });
        } else {
            openAnotacoesBody("body-anotacoes-list-disciplinas");
        }
    }
</script>
        <div class="panel-body" id="body-anotacoes-primeira-anotacao">
          <div id="primeira-anotacao-content">
            <i class="fa fa-file-text-o"></i>
            <h1>Faça sua primeira anotação</h1>
            <p>Você ainda não tem uma anotação</p>
          </div>
          <button id="primeira-anotacao-button" class="btn btn-primary"
            onclick="openAnotacoesBody('body-anotacoes-formulario');">Iniciar
            anotação</button>
        </div>
        <div class="panel-body" id="body-anotacoes-formulario">
          <input type="hidden" id="anotacoes-disciplina-id">
          <input type="hidden" id="anotacoes-disciplina-nome">
          <a class="anotacoes-form-title" id="anotacoes-form-title" href="#">
            <i class="fa fa-chevron-left"></i>
            <h2 id="anotacoes-form-title-h2"></h2>
          </a>
          <div id="anotacoes-form-list" class="anotacoes-form-list">

          </div>
        </div>
        <script>
    function clearListAnotacoes() {
        var lista = document.getElementById("anotacoes-form-list")
        lista.innerHTML =
            "<div class='anotacoes-list-item'>" +
            "<textarea id='anotacoes-form-textarea'></textarea>" +
            "<input type='hidden' id='anotacoes-form-anotacaoId'>" +
            "<button class='send-anotacao-form btn btn-primary' id='send-anotacao-form-btn' onclick='saveAnotacao()'>Incluir Anotação</button>" +
            "</div>";
    }
    function loadListAnotacoes() {
        const matriculaId = document.getElementById("anotacoes-matricula-id").value;
        const disciplinaId = document.getElementById("anotacoes-disciplina-id").value;

        $.ajax({
            url: hostname + "aluno/anotacao",
            type: 'GET',
            dataType: 'json',
            data: {matriculaId: matriculaId, disciplinaId: disciplinaId},
            success: function(retorno){
                clearListAnotacoes();
                var lista = document.getElementById("anotacoes-form-list");

                if (retorno.length == 0) {
                    openAnotacoesBody('body-anotacoes-primeira-anotacao');
                } else {
                    retorno.forEach(function (el) {
                        lista.innerHTML +=
                            "<div class='anotacoes-list-item'>" +
                                "<p class='anotacao-content'>" + el.anotacao + "</p>" +
                                "<div class='anotacao-action-links'>" +
                                    "<a href=\"#\" onclick=\"initEditarAnotacao('" + el._id + "', '" + el.anotacao + "')\" class=\"editar-anotacao\">Editar</a>" +
                                    "<a href=\"#\" onclick=\"deleteAnotacao('" + el._id + "')\" class=\"excluir-anotacao\">Excluir</a>" +
                                "</div>" +
                            "</div>";
                    });

                    openAnotacoesBody('body-anotacoes-formulario');
                }
            },
            error: function(retorno){
                console.error('error', retorno);
                openAnotacoesBody('body-anotacoes-erro');
                clearListAnotacoes();
            }
        });
    }

    function openAnotacoesFormulario(disciplinaId, disciplinaNome, telaTimeline) {
        document.getElementById("anotacoes-disciplina-id").value = disciplinaId;
        document.getElementById("anotacoes-disciplina-nome").value = disciplinaNome;
        document.getElementById("anotacoes-form-title-h2").innerHTML = disciplinaNome;

        document.getElementById("anotacoes-form-title").onclick = function () {
            closeAnotacoesFormulario(telaTimeline);
        };

        loadListAnotacoes();
    }
    function closeAnotacoesFormulario(telaTimeline) {
        if (telaTimeline)
            closeNavAnotacoes();
        else
            openAnotacoesBody('body-anotacoes-list-disciplinas');
        clearListAnotacoes();
    }

    function initEditarAnotacao(anotacaoId, anotacao) {
        document.getElementById("anotacoes-form-anotacaoId").value = anotacaoId;
        document.getElementById('send-anotacao-form-btn').onclick = updateAnotacao;
        document.getElementById('send-anotacao-form-btn').innerText = 'Salvar';
        document.getElementById("anotacoes-form-textarea").value = anotacao;
        document.getElementById("anotacoes-form-textarea").focus();

    }

    function saveAnotacao(){
        const matriculaId = document.getElementById("anotacoes-matricula-id").value;
        const disciplinaId = document.getElementById("anotacoes-disciplina-id").value;
        const disciplinaNome = document.getElementById("anotacoes-disciplina-nome").value;
        const anotacao = document.getElementById("anotacoes-form-textarea").value.trim();

        if (!matriculaId || !disciplinaId || ! disciplinaNome || !anotacao) return

        document.getElementById('send-anotacao-form-btn').disabled = true;

        var payload = {
            matriculaId: matriculaId,
            disciplinaId: disciplinaId,
            disciplinaNome: disciplinaNome,
            anotacao: anotacao
        };
        $.ajax({
            url: hostname + "aluno/anotacao/save",
            type: 'POST',
            dataType: 'json',
            contentType: "application/json",
            data: JSON.stringify(payload),
            success: function(retorno){
                loadListAnotacoes();
            },
            error: function(retorno){
                console.error('error', retorno);
                openAnotacoesBody('body-anotacoes-erro');
                clearListAnotacoes();
            }
        });
    }
    function updateAnotacao(){
        const anotacaoId = document.getElementById("anotacoes-form-anotacaoId").value;
        const anotacao = document.getElementById("anotacoes-form-textarea").value.trim();

        if (!anotacaoId || !anotacao) return

        document.getElementById('send-anotacao-form-btn').disabled = true;

        var payload = {id: anotacaoId, anotacao: anotacao};
        $.ajax({
            url: hostname + "aluno/anotacao/edit",
            type: 'PUT',
            dataType: 'json',
            contentType: "application/json",
            data: JSON.stringify(payload),
            success: function(retorno){
                loadListAnotacoes()
            },
            error: function(retorno){
                console.error('error', retorno);
                openAnotacoesBody('body-anotacoes-erro');
                clearListAnotacoes();
            }
        });
    }
    function deleteAnotacao(anotacaoId){
        const userConfirm = confirm('Você realmente deseja excluir a anotação?');
        if (!userConfirm) return;

        var payload = {id: anotacaoId};
        $.ajax({
            url: hostname + "aluno/anotacao/delete",
            type: 'DELETE',
            dataType: 'json',
            contentType: "application/json",
            data: JSON.stringify(payload),
            success: function(retorno){
                loadListAnotacoes()
            },
            error: function(retorno){
                console.error('error', retorno);
                openAnotacoesBody('body-anotacoes-erro');
                clearListAnotacoes();
            }
        });
    }
</script>

        <div class="panel-body" id="body-anotacoes-erro">
          <div id="anotacoes-erro-content">
            <i class="fa fa-times-circle-o"></i>
            <h1>Serviço indisponivel no momento</h1>
            <p>Por favor, tente mais tarde!</p>
          </div>
        </div>
      </div>
    </div>
    <script>
    function _openNavAnotacoes() {
        document.getElementById("mySidenavAnotacoes").classList.add('sidenavAnotacoesActive');
        document.getElementById("mascaraAnotacoes").classList.add('mascaraAnotacoesActive');
    }
    function openNavAnotacoes(matriculaId, disciplinaId, disciplinaNome) {
        document.getElementById("anotacoes-matricula-id").value = matriculaId;

        if (disciplinaId && disciplinaNome)
            openAnotacoesFormulario(disciplinaId, disciplinaNome, true);
        else
            openAnotacoesListDisciplinas();
    }

    function closeNavAnotacoes() {
        document.getElementById("anotacoes-matricula-id").value = '';

        document.getElementById("mySidenavAnotacoes").classList.remove('sidenavAnotacoesActive');
        document.getElementById("mascaraAnotacoes").classList.remove('mascaraAnotacoesActive');

        clearAllAnotacoesBody();
    }

    function clearAllAnotacoesBody() {
        document.querySelectorAll('#mySidenavAnotacoes .panel-body').forEach(body =>
            body.classList.remove('anotacoes-body-active')
        );
    }

    function openAnotacoesBody(bodyName) {
        clearAllAnotacoesBody();
        document.getElementById(bodyName).classList.add('anotacoes-body-active');
        _openNavAnotacoes();
    }
</script>

  </html>
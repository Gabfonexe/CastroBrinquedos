$('#termochk').change(function() {
	if($(this).is(":checked")) {
		$("#btnProsseguir").prop("disabled", false);
	} else {
		$("#btnProsseguir").prop("disabled", true);
	}
});

var popupList = [];
$('.js-popup-item').each(function(){
	popupList.push($(this).attr('id'));
});

function popupAva() {
	if (popupList.length > 0) {
		$.magnificPopup.open({
			items: {
				src: "#" + popupList[0],
				type: "inline"
			},
			closeBtnInside: false,
			mainClass: 'mfp-inline-popup',
			callbacks: {
				open: function() {
					popupList.shift();

				},
				afterClose: function() {
					popupAva();
				}
			}
		});
	}  else {
        var urlX = "";
        if (urlX != '') {
            window.location.href = window.location.protocol + "//" + window.location.host + "";
        }
    }
}


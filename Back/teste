      <script>
        var divContas = $('#divContas');

        $('#form-alternar').submit(function (e) {
            e.preventDefault();
        });

        function desabilitaBtnForm() {
            $('#btn-submit-alternar').addClass('disabled');
        }

        function habilitaBtnForm() {
            $('.js-input-username').on('click', function () {
                $('#btn-submit-alternar').removeClass('disabled');
            });
        }

        function desabilitarBotoesAlternarUsuario(){
            $('#btn-submit-alternar').addClass('disabled');
            $('#btn-cancelar-alternar').addClass('disabled');
            $('#btnAlternar').addClass('disabled');
        }

        $('#btn-submit-alternar').on('click', function () {
            var contaDestino = $('input[name="username"]:checked').val();
            var usuarioOrigem = $('#idUsuarioOrigem').val();
            desabilitarBotoesAlternarUsuario();
            $('#js-spinner').show();
            $.ajax({
                url: '/impersonateLog/salvarLogImpersonate',
                type: 'GET',
                dataType: 'json',
                //    async: true,
                data: {contaDestino: contaDestino, usuarioOrigem: usuarioOrigem},
                complete: function() {
                    $('#form-submit-alternar').submit();
                }
            });
        });



        $('#btnAlternar').on('click', function () {
            desabilitaBtnForm();
            var conta = $('#usernameSwitch').val();
            limparCampoContas();
            if (!conta.length || conta.length < 3) {
                setTimeout(function () {
                    aparecerCampoContas("Por favor, insira pelo menos 3 caracteres para realizar a pesquisa.", "alert-warning");
                }, 300);
                return;
            }

            $.ajax({
                url: '/usuario/getContaSwitchUser',
                type: 'GET',
                dataType: 'json',
                async: true,
                data: {conta: conta},
                error: function () {
                    setTimeout(function () {
                        aparecerCampoContas("Algo deu errado, tente novamente.", "alert-warning");
                    }, 300);
                },
                success: function (result) {
                    if (result.length > 0) {

                        divContas.append("<h4>Contas:</h4>");
                        for (let i = 0; i < result.length; i++) {
                            constroiTabelaContas(result[i]);
                        }
                        habilitaBtnForm();
                        divContas.attr('role', 'alert');
                        divContas.addClass("alert");
                        divContas.addClass("alert-success");
                        divContas.fadeIn(450);
                    } else {
                        setTimeout(function () {
                            aparecerCampoContas("Nenhuma conta encontrada.", "alert-warning");
                        }, 300);
                    }
                }
            });
        });

        function constroiTabelaContas(item) {
            divContas.append("<li class='radio'><label for='" + item.conta + "'><input type='radio' class='js-input-username' name='username' id=" + item.conta + " value='" + item.conta + "'>" + item.conta + "</label></li>");
        }

        function aparecerCampoContas(text, classe) {
            divContas.attr('role', 'alert');
            divContas.addClass(classe);
            divContas.html(text);
            divContas.fadeIn(450);
        }

        function limparCampoContas() {
            divContas.fadeOut(450);
            divContas.removeClass('alert-warning');
            divContas.removeClass('alert-success');
            divContas.html('');
        }

        function getUsuarios(conta) {
            console.log("OnChange");
            if (!conta.length || conta.length < 3) return callback();

        }
    </script>

